{
  "name": "artifact-viewer",
  "displayName": "Artifact Viewer",
  "description": "View artifacts generated by AI Agents",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:artifact-viewer.view"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "artifact-viewer.view",
        "title": "View Artifact"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.107.0",
    "typescript": "^6.0.0-dev"
  }
}
