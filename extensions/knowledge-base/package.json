{
  "name": "knowledge-base",
  "displayName": "Knowledge Base",
  "description": "A knowledge base for AI Agents",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:knowledge-base-view"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "knowledge-base",
          "title": "Knowledge Base",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "knowledge-base": [
        {
          "id": "knowledge-base-view",
          "name": "Knowledge Base",
          "type": "webview"
        }
      ]
    },
    "commands": [
      {
        "command": "knowledge-base.search",
        "title": "Search Knowledge Base"
      },
      {
        "command": "knowledge-base.add",
        "title": "Add to Knowledge Base"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "dependencies": {
    "level": "^8.0.0"
  },
  "devDependencies": {
    "@types/vscode": "^1.107.0",
    "typescript": "^6.0.0-dev"
  }
}
